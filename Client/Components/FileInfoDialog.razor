 <MudDialog>
    <DialogContent>
        File ID: @Pages.Index.fileDescriptor.FileID
        <br />
        Key: @Pages.Index.fileDescriptor.Key.GetIV().ToString();
        </DialogContent>
    <DialogActions>
     <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code{
    
     [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}