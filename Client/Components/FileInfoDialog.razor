 <MudDialog Style="width=60%">
    <DialogContent>
        File ID: <MudTextField @bind-Value=@Pages.Index.fileDescriptor.FileID/>
        <br />
        Key: <MudTextField @bind-Value=@Pages.Index.fileDescriptor.KeyString/>
        </DialogContent>
    <DialogActions>
     <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code{
    
     [CascadingParameter] MudDialogInstance MudDialog { get; set; }
    void Submit() => MudDialog.Close(DialogResult.Ok(true));
}